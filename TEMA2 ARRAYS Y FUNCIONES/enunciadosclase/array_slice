<?php

$new=[];
for ($i=0;$i<20;$i++){
$aleat=mt_rand(0,9);
array_push($new,$aleat);
}
sort($new);
print_r($new);
php_eol;
$primera=array_slice($new,0,-10);
$segunda=array_slice($new,10);
print_r($primera);
print_r($segunda);
php_eol;
for($i=0;$i<count($primera);$i++){
	array_push($segunda,$primera[$i]);
}
print_r($segunda);

?>
